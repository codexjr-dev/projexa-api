{
  "name": "projexa",
  "version": "1.4.0",
  "description": "API do gerenciador de EJs",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=dev nodemon src/server.js",
    "debug": "NODE_ENV=dev nodemon --inspect src/server.js",
    "test": "NODE_ENV=test mocha ./test/integration/*.test.js --timeout 10000 --exit",
    "start:win32": "set NODE_ENV=dev && nodemon src/server.js",
    "debug:win32": "set NODE_ENV=dev && nodemon --inspect src/server.js",
    "test:win32": "set NODE_ENV=test && mocha ./test/integration/*.test.js --timeout 10000 --exit",
    "deploy": "NODE_ENV=prod node src/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codexjr-dev/dashboard-codex-api.git"
  },
  "author": "CodeX Jr.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codexjr-dev/dashboard-codex-api/issues"
  },
  "homepage": "https://github.com/codexjr-dev/dashboard-codex-api#readme",
  "dependencies": {
    "@types/cors": "^2.8.19",
    "bcrypt": "6.0.0",
    "body-parser": "2.2.0",
    "cors": "^2.8.5",
    "dotenv": "17.2.1",
    "express": "5.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "2.2.3",
    "mongoose": "8.17.2",
    "nodemailer": "7.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "chai": "5.3.1",
    "chai-http": "5.1.2",
    "mocha": "11.7.1",
    "mongodb-memory-server": "10.2.0",
    "nodemon": "3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "_moduleAliases": {
    "@": "src",
    "@middlewares": "src/middlewares",
    "@user": "src/modules/User",
    "@ej": "src/modules/Ej",
    "@project": "src/modules/Project",
    "@link": "src/modules/Link",
    "@member": "src/modules/Member",
    "@news": "src/modules/News",
    "@config": "src/config"
  }
